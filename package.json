{
  "name": "landscaper",
  "description": "CodeMods as a Service",
  "version": "0.0.2",
  "author": "Bitovi",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "nyc": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "bin": {
    "landscaper": "./lib/cli.js",
    "lc": "./bin/landscaper.js"
  },
  "bugs": {
    "url": "https://github.com/bitovi/landscaper/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "chalk": "^2.0.1",
    "globby": "^6.1.0",
    "inquirer": "^3.0.6",
    "jscodeshift": "^0.3.30",
    "npm-programmatic": "0.0.8",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "del": "^2.2.2",
    "fixpack": "^2.3.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^2.6.0",
    "nyc": "^10.3.0",
    "source-map-support": "^0.4.15",
    "standard": "^10.0.2"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/bitovi/landscaper#readme",
  "keywords": [
    "codemods",
    "mod",
    "transforms"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "git+https://github.com/bitovi/landscaper.git",
  "scripts": {
    "//prepublish": "npm run build && npm run test",
    "build": "gulp",
    "cli": "npm run landscaper",
    "coveralls": "npm run report && nyc report --reporter=text-lcov | coveralls",
    "lint": "standard --fix && fixpack",
    "report": "NODE_ENV=nyc nyc npm test",
    "test": "npm run lint && ava --serial",
    "test-ci": "npm run prepublish",
    "watch": "gulp watch"
  }
}
